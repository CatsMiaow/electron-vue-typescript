{
  "name": "electron-vue-typescript",
  "version": "1.0.0",
  "description": "Electron Vue.js Structure for TypeScript Example",
  "main": "app/dist/main/app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext .ts .",
    "build:main": "cross-env NODE_ENV=production webpack --config config/webpack.main.js --progress --profile --bail",
    "build:renderer": "cross-env NODE_ENV=production webpack --config config/webpack.renderer.prod.js --progress --profile --bail",
    "dev:main": "cross-env NODE_ENV=development webpack --config config/webpack.main.js --progress --profile --bail && electron config/electron.dev.js",
    "dev:renderer": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.renderer.dev.js --inline --progress --profile --port 3000",
    "build": "npm run build:renderer && npm run build:main",
    "pack": "electron-builder --dir -mw --x64",
    "dist": "electron-builder -mw --x64"
  },
  "devDependencies": {
    "@mdi/font": "^5.0.45",
    "@types/node": "^13.11.0",
    "@types/vuelidate": "^0.7.11",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "dtslint": "^3.4.1",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-sonarjs": "^0.5.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.0.4",
    "material-design-icons-iconfont": "^5.0.1",
    "raw-loader": "^4.0.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "typescript": "^3.8.3",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.6",
    "vuelidate": "^0.7.5",
    "vuetify": "^2.2.20",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CatsMiaow/electron-vue-typescript.git"
  },
  "keywords": [
    "Electron",
    "TypeScript",
    "Vue",
    "Vuejs",
    "Webpack"
  ],
  "homepage": "https://github.com/CatsMiaow/electron-vue-typescript#readme",
  "author": "CatsMiaow",
  "license": "MIT"
}
